    "icons": {
        "16": "/icon/16.png",
        "32": "/icon/32.png",
        "48": "/icon/48.png",
        "128": "/icon/128.png"
    },
    "manifest_version": 3,
    "minimum_chrome_version": "88.0.0.0",
    "name": "防沉迷终结者 beta",
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "declarativeNetRequestWithHostAccess",
        "notifications",
        "storage"
    ],
    "version": "1.2",
    "version_name": "1.2 beta",
    "web_accessible_resources": [
        {
            "matches": [
                "*://*.17yy.com/*",
                "*://*.4399.com/*",
